{
  "name": "anonygram-server",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "preinstall": "node gcpSetup.js",
    "test": "mocha --require test/setup.js",
    "migrate": "postgrator --config postgrator-config.js",
    "migrate:test": "env NODE_ENV=test npm run migrate",
    "migrate:prod": "env SSL=true DATABASE_URL=$(heroku config:get DATABASE_URL) npm run migrate",
    "seed": "psql -f ./seeds/seed.tables.sql -d anonygram",
    "seed:prod": "psql -f ./seeds/seed.tables.sql -d \"$(heroku config:get DATABASE_URL)\"",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "predeploy": "npm audit",
    "deploy": "git push heroku master"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/vision": "^1.7.2",
    "aws-sdk": "^2.579.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.17.1",
    "helmet": "^3.21.1",
    "jsonwebtoken": "^9.0.0",
    "knex": "^0.20.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "pg": "^7.14.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.8",
    "sharp": "^0.23.3",
    "winston": "^3.2.1",
    "xss": "^1.0.6"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-fs": "^2.0.0",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "postgrator-cli": "^3.3.0",
    "supertest": "^4.0.2"
  }
}
